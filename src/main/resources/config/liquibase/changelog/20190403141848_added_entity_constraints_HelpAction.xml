        <addForeignKeyConstraint baseColumnNames="from_id"
                                 baseTableName="help_action"
                                 constraintName="fk_help_action_from_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
